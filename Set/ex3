#include <iostream>
#include <vector>
#include <set>

bool IsSubset(const std::vector<int>& subset , const std::vector<int>& superset){
    std::set<int> unique(superset.begin() , superset.end());
   
    for (int x : subset ) {
        if(unique.count(x) == 0){ 
            return false;
        }
    }
    return true;
}
  

    int main()
{
    std::vector<int> subset = {1, 3, 5};
    std::vector<int> superset = {1, 2, 3, 4, 5};
    std::cout << IsSubset(subset, superset) << '\n';  // 1 (true)
    
    std::vector<int> subset2 = {1, 6};
    std::cout << IsSubset(subset2, superset) << '\n';  // 0 (false)

    return 0;
}
