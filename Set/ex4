#include <iostream>
#include <vector>
#include <set>

std::vector<int> findMissingNumbers(const std::vector<int>& nums , const int& N){
    std::set<int> unique(nums.begin() , nums.end());
    std::vector<int> num;
    for (int i = 1 ; i <= N ; i++) {
        if(unique.count(i) == 0) {
            num.push_back(i);
        }
    }
   
    return num;
}
  

    int main()
{
   std::vector<int> nums = {1, 3, 5, 6};
    auto result = findMissingNumbers(nums, 6);
    // result: {2, 4}
    for (int& x : result ) {
        std::cout << x << " ";
    }
    return 0;
}
