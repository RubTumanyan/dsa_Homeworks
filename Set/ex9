#include <iostream>
#include <vector>
#include <set>
#include <algorithm>

int removeRange(std::set<int>& st , int low ,int high) {
    auto low_st = st.lower_bound(low);
    auto higt_st = st.upper_bound(high);

    
    int cont = std::distance(low_st , higt_st);
    st.erase(low_st, higt_st);
    return cont;
}

int main() {
    
    std::set s = {1, 3, 5, 7, 9, 11, 13, 15};
    int removed = removeRange(s, 5, 11);
    std::cout << removed << '\n';  // 4
    // s теперь содержит: {1, 3, 13, 15}

    
    return 0;
}

