#include <iostream>
#include <stack>

bool isBalanced(const std::string& s) {
    std::stack<char> stack;

    for (char c : s) {
        if (c == '(' || c == '{' || c == '[') {
            stack.push(c);
        }

        else if (c == ')' || c == '}' || c == ']') {
            if (stack.empty()) {
                return false;
            }

            char top = stack.top();
            stack.pop();

            if ((c == ')' &&  top != '(' ) || (c == '}' && top != '{') || (c == ']' && top != '[')) {
                return false;
            }
        }
        }
    return true;
    }
int main() {
    std::cout << std::boolalpha << isBalanced("({[]})") << '\n';
    std::cout << isBalanced("([)]") << '\n';
    return 0;
}
