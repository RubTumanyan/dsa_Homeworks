#include <iostream>
#include <forward_list>

void mergeSortedLists(std::forward_list<int>& num , std::forward_list<int>& num1 ) {
    auto it = num.before_begin();
    auto it_1 = num.begin();
    auto it_2 = num1.begin();
    while(it_2 != num1.end()) {
        while(it_1 != num.end() && *it_1 <= *it_2) {
            it_1++;
            it++;
        }
        it_1 = num.insert_after( it , *it_2 );
        it_2++;
       }
       num1.clear();
    }


void printList(std::forward_list<int>& list) {
    for (int elem : list) {
        std::cout << elem << " ";
    }
}

int main() {
   
   std::forward_list<int> fl1 = {1, 3, 5};
   std::forward_list<int> fl2 = {2, 4, 6};
   mergeSortedLists(fl1,fl2);
   printList(fl1);
    return 0;
}
