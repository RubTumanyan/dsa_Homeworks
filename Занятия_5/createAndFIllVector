#include <iostream>
#include <vector>
std::vector<int> createAndFillVector(int N) {
    std::vector<int> result(N);
    for (int i = 1 ; i <= result.size() ; i++) {
        result[i-1] = i;
    }
    return result;
}
int main()
{
    int m;
    std::cin >> m;
    
    std::vector<int> result = createAndFillVector(m); 
    for ( int &x : result ) {
        std::cout << x << " ";
    }
    return 0;
}
