#include <iostream>
#include <vector>

int findSubsequence(std::vector<int>& vec, std::vector<int>& vec1) {
    
    if(vec.size() < vec1.size()) {
        return -1;
    }
    int i = 0 , j = 0;
    for (int i = 0 ; i <= vec.size() - vec1.size() ; i++) {
        bool yes = true;
        for (int j = 0 ; j < vec1.size() ; j++) {
            if (vec[i + j] != vec1[j]) {
                yes = false;
                break;
            }
        }
        if (yes) {
            return i;
        }
    }
    return -1;
}    
int main()
{
    std::vector<int> vec = {4 , 5 , 8, 16};
    std::vector<int> vec1 = { 8 , 16};
    int i = findSubsequence(vec , vec1);
    std::cout << i ;

    return 0;
}
