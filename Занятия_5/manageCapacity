#include <iostream>
#include <vector>

void manageCapacity(std::vector<int>& vec){
    std::cout << vec.size() << " " << vec.capacity() << std::endl;
    
    vec.reserve(vec.size() + 500);
    for (int i = 0 ; i <= 500 ; i++) {
        vec.push_back(i);
    }
        std::cout << vec.size() << " " << vec.capacity() << std::endl;

}
int main()
{
    std::vector<int> vec = {4 , 5 , 2, 5};
    manageCapacity(vec);

    return 0;
}
