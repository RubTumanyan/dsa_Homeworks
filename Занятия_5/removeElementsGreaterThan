
#include <iostream>
#include <vector>
int removeElementsGreaterThan(std::vector<int>& vec , int count) {
    int count1 = 0;
    for (int i = vec.size() -1 ;  i >= 0 ; i--) {
        if (vec[vec.size() - 1] > count) {
            vec.pop_back();
            count1++;
        }
    }
    return count1;
}
int main()
{
    
    std::vector<int> result = {1,5,10,25,1000,121265};
    std::cout << removeElementsGreaterThan(result , 30 );
    
    return 0;
}
